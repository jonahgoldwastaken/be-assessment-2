{
  "name": "hdate",
  "version": "1.0.0",
  "description": "A dating site for hobbyists",
  "private": true,
  "scripts": {
    "prestart": "mongod --dbpath db --fork --logpath db/log/mongo.log && yarn build",
    "start": "node .",
    "stop": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "poststop": "rm db/log/*",
    "prewatch": "mongod --dbpath db --fork --logpath db/log/mongo.log",
    "watch": "nodemon .",
    "build": "webpack-cli --config webpack.config.js --hide-modules",
    "clean-up": "rm -r assets && rm db/log/*",
    "lint": "eslint *.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theonejonahgold/hdate.git"
  },
  "keywords": [],
  "author": "Jonah Meijers <jonahmeijers97@gmail.com> (https://jonahmeijers.nl)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theonejonahgold/hdate/issues"
  },
  "homepage": "https://github.com/theonejonahgold/hdate#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "babel-loader": "^8.0.0-beta",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "nodemon": "^1.17.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "mongodb": "^3.0.5"
  }
}
